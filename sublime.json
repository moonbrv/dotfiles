{
	"theme": "Adaptive.sublime-theme",
	"ignored_packages":
	[
		"Vintage",
	],
	"file_exclude_patterns": ["*.pyc", "*.pyo", "*.exe", "*.dll", "*.obj","*.o", "*.a", "*.lib", "*.so", "*.dylib", "*.ncb", "*.sdf", "*.suo", "*.pdb", "*.idb", ".DS_Store", ".directory", "desktop.ini", "*.class", "*.psd", "*.db", "*.sublime-workspace"],
	"color_scheme": "Mariana.sublime-color-scheme",
	"line_numbers": false,
	"gutter": false,
	"font_size": 12,
	"tab_size": 2,
	"translate_tabs_to_spaces": true,
	"trim_trailing_white_space_on_save": "all",
	"ensure_newline_at_eof_on_save": true,
	"draw_white_space": "all",
	"highlight_modified_tabs": true,
	"hide_new_tab_button": true,
	"remember_layout": true,
	"folder_exclude_patterns": [".svn", ".git", ".hg", "CVS", ".Trash", ".Trash-*", ".lsp", ".vscode", "__pycache__", ".pytest_cache", "node_modules", "venv", ".idea", ".shadow-cljs",  ".clj-kondo/cache", ".clj-kondo/.cache", ".calva", "**/target/**", "**/compiled/**", "**/sorbet/**", ".next"],
	"index_exclude_patterns": ["*.log", ".svn", ".git", ".hg", "CVS", ".Trash", ".Trash-*", ".lsp", ".vscode", "__pycache__", ".pytest_cache", "node_modules", "venv", ".idea", ".shadow-cljs",  ".clj-kondo/cache", ".clj-kondo/.cache", ".calva", "**/target/**", "**/compiled/**", "**/sorbet/**", ".next"],
	"goto_anything_exclude_gitignore": true,
}
